# Environment Variables Example
# Copy this file to .env and update the values

# Base URLs
BASE_URL=https://demo.playwright.dev
DEFAULT_API_URL=https://reqres.in/api

# Authentication
LOGIN_USERNAME=test_user
LOGIN_PASSWORD=test_password

# External Resources
AXE_CORE_CDN=https://cdnjs.cloudflare.com/ajax/libs/axe-core/4.7.0/axe.min.js
CHART_JS_CDN=https://cdn.jsdelivr.net/npm/chart.js
DEFAULT_EMAIL_DOMAIN=example.com

# Xray Integration
XRAY_API_URL=https://xray.cloud.getxray.app/api/v2
XRAY_CLIENT_ID=your_client_id
XRAY_CLIENT_SECRET=your_client_secret
XRAY_PROJECT_KEY=TEST

# OAuth Configuration
OAUTH_TOKEN_URL=https://oauth.example.com/token
CLIENT_ID=your_client_id
CLIENT_SECRET=your_client_secret